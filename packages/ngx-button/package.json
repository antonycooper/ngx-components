    "node-sass": "^4.5.3",
    "postcss-loader": "^1.3.3",
    "precss": "^2.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "stylelint": "^7.12.0",
    "stylelint-webpack-plugin": "^0.8.0",
    "to-string-loader": "^1.1.5",
    "ts-node": "^3.1.0",
    "tslib": "^1.7.1",
    "tslint": "~5.4.3",
    "tslint-loader": "~3.5.3",
    "typescript": "~2.4.1",
    "webpack": "~3.1.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "~2.5.0",
    "webpack-merge": "~4.1.0"
  }
}
