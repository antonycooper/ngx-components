{
  "name": "francium-ngx-dashboard",
  "applicationName": "ClearScore",
  "version": "1.0.0",
  "description": "Clearscore Report dashboard",
  "scripts": {
    "build:aot": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "build": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "ci:travis": "npm run test && npm run build:aot",
    "clean:dist": "npm run rimraf -- dist",
    "prestart": "npm run clean:dist",
    "rimraf": "rimraf",
    "server": "webpack-dev-server --progress --port 3020 --open --content-base src",
    "server:local": "live-server dist --cors --host=localhost --port=3020 --entry-file=index.html",
    "start": "npm run server",
    "test": "npm run lint && karma start",
    "test:reload": "npm run lint && karma start --single-run=false",
    "watch": "npm run build -- --watch",
    "tslint": "tslint",
    "lint": "npm run tslint 'src/**/*.ts'",
    "compodoc": "compodoc src -p tsconfig.json -s -d reports",
    "clean-workspace": "scripts/clean-workspace.sh",
    "pack-main": "scripts/pack-main.sh",
    "pack-ansible": "scripts/pack-ansible.sh",
    "publish-artifacts": "scripts/publish-artifacts.sh"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~4.3.0",
    "@angular/common": "~4.3.0",
    "@angular/compiler": "~4.3.0",
    "@angular/core": "~4.3.0",
    "@angular/forms": "~4.3.0",
    "@angular/http": "~4.3.0",
    "@angular/platform-browser": "~4.3.0",
    "@angular/platform-browser-dynamic": "~4.3.0",
    "@angular/platform-server": "~4.3.0",
    "@angular/router": "~4.3.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/router-store": "^1.2.6",
    "@ngrx/store": "^2.2.2",
    "@ngrx/store-devtools": "^3.2.4",
    "@ngx-translate/core": "^7.0.0",
    "@ngx-translate/http-loader": "0.1.0",
    "core-js": "^2.4.1",
    "ie-shim": "^0.1.0",
    "live-server": "~1.2.0",
    "moment": "^2.18.1",
    "ngx-cookie": "^1.0.0",
    "progressbar.js": "^1.0.1",
    "rxjs": "~5.4.2",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~4.3.0",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.53",
    "@types/moment": "^2.13.0",
    "@types/node": "^8.0.7",
    "@types/selenium-webdriver": "~3.0.4",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.29",
    "@types/webpack": "^3.0.1",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "^7.1.1",
    "awesome-typescript-loader": "~3.2.1",
    "codelyzer": "~3.1.1",
    "compodoc": "0.0.41",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "html-minifier": "^3.5.2",
    "html-webpack-plugin": "^2.29.0",
    "istanbul-instrumenter-loader": "~2.0.0",
    "jasmine-core": "^2.6.4",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.3",
    "ng-router-loader": "^2.1.0",
